
<%= form_with model: @student do |i|%>
<div>
    <%= i.label :first_name %><br>
    <%= i.text_field :first_name%>
</div>
<div>
    <%= i.label :last_name %><br>
    <%= i.text_field :last_name%>
</div>
<div>
    <%= i.label :email %><br>
    <%= i.text_field :email%>
</div>
<div>
    <%= i.label :date_of_birth %><br>
    <%= i.text_field :date_of_birth%>
</div>
<%= i.submit%>
<% end %> 
